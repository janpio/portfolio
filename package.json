{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">= 18.15.0 < 19.0.0",
    "pnpm": ">= 8.3.1 < 9",
    "yarn": "please-use-pnpm",
    "npm": "please-use-pnpm"
  },
  "scripts": {
    "build": "prisma generate && prisma migrate deploy && next build",
    "clean": "rimraf .next && rimraf pnpm-lock.yaml && rimraf .contentlayer && rimraf node_modules",
    "dev": "next dev",
    "db:push": "pnpm --package=dotenv-cli dlx dotenv -e .env.local -- prisma db push",
    "db:seed": "pnpm --package=dotenv-cli dlx dotenv -e .env.local -- prisma db seed",
    "format": "prettier --write src/",
    "lint": "next lint",
    "migrate:dev": "pnpm --package=dotenv-cli dlx dotenv -e .env.local -- prisma migrate dev",
    "migrate:reset": "pnpm --package=dotenv-cli dlx dotenv -e .env.local -- prisma migrate reset",
    "migrate:deploy": "pnpm --package=dotenv-cli dlx dotenv -e .env.local -- prisma migrate deploy",
    "prisma:generate": "pnpm --package=dotenv-cli dlx dotenv -e .env.local -- prisma generate",
    "prisma:studio": "pnpm --package=dotenv-cli dlx dotenv -e .env.local -- prisma studio",
    "start": "next start"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "4.15.0",
    "@tailwindcss/typography": "0.5.9",
    "@vercel/analytics": "1.0.1",
    "contentlayer": "0.3.3",
    "date-fn": "0.0.2",
    "date-fns": "2.30.0",
    "framer-motion": "10.12.16",
    "lodash": "4.17.21",
    "lottie-react": "2.4.0",
    "next": "13.4.6",
    "next-contentlayer": "0.3.3",
    "react": "18.2.0",
    "react-cmdk": "1.3.9",
    "react-dom": "18.2.0",
    "react-icons": "4.9.0",
    "react-tooltip": "5.14.0",
    "rehype-autolink-headings": "6.1.1",
    "rehype-pretty-code": "0.9.8",
    "rehype-slug": "5.1.0",
    "remark-gfm": "3.0.1"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.0.2",
    "@types/lodash": "4.14.195",
    "@types/node": "20.3.1",
    "@types/react": "18.2.12",
    "@types/react-dom": "18.2.5",
    "autoprefixer": "10.4.14",
    "eslint": "8.42.0",
    "eslint-config-next": "13.4.6",
    "eslint-config-prettier": "8.8.0",
    "postcss": "8.4.24",
    "prettier": "2.8.8",
    "prettier-plugin-jsdoc": "0.4.2",
    "prettier-plugin-tailwindcss": "0.3.0",
    "prisma": "4.15.0",
    "rimraf": "5.0.1",
    "sass": "1.63.4",
    "tailwindcss": "3.3.2",
    "ts-node": "10.9.1",
    "typescript": "5.1.3"
  },
  "pnpm": {
    "packageExtensions": {
      "contentlayer": {
        "peerDependencies": {
          "esbuild": "0.17.19"
        }
      }
    }
  }
}
